class Solution {
public:
    int minMovesToSeat(vector<int>& seats, vector<int>& students) {
        int n = seats.size();
        int m = students.size();

        sort(seats.begin(),seats.end());
        sort(students.begin(),students.end());

        int ans =0;
        int i=0;
        while (i<n)
        {
            ans += abs(seats[i] - students[i]);
            i++;
        }

        return ans;
    }
};
